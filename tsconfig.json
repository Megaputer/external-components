{
  "compilerOptions": {
    "target": "es6",
    "module": "AMD",
    "sourceMap": true,
    "removeComments": false,
    "moduleResolution": "node",
    "importHelpers": true,
    "noEmitHelpers": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "./src",
    "jsx": "react",
    "typeRoots": ["./node_modules/@types", "./src/@types", "./node_modules/pa-typings"]
  },
  "include": ["src"]
}
